local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

getgenv().users = {
	Main = "0xsedative",
	Alt1 = "HitTalker3",
	Alt2 = "hitTalker2"
}

local KnifeName = "Knife"
local KnifeEquipped = false
local Anchored = false

-- Knife'ı eline alma fonksiyonu
local function equipKnife()
	local backpack = LocalPlayer:FindFirstChild("Backpack")
	local char = LocalPlayer.Character
	if backpack and backpack:FindFirstChild(KnifeName) then
		backpack[KnifeName].Parent = char
	end
end

-- Oyuncuyu LocalPlayer önüne ışınla
local function teleportOthers()
	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and not table.find(getgenv().users, player.Name) and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
			if hrp then
				player.Character.HumanoidRootPart.CFrame = hrp.CFrame * CFrame.new(0, 0, -1)
			end
		end
	end
end

-- Knife kontrol döngüsü
RunService.RenderStepped:Connect(function()
	local char = LocalPlayer.Character
	local backpack = LocalPlayer:FindFirstChild("Backpack")
	local knifeInChar = char and char:FindFirstChild(KnifeName)
	local knifeInBackpack = backpack and backpack:FindFirstChild(KnifeName)

	if knifeInChar or knifeInBackpack then
		if not KnifeEquipped then
			equipKnife()
			KnifeEquipped = true
		end

		if char and char:FindFirstChild("HumanoidRootPart") and not Anchored then
			char.HumanoidRootPart.Anchored = true
			Anchored = true
		end

		teleportOthers()
	else
		KnifeEquipped = false
		if Anchored and char and char:FindFirstChild("HumanoidRootPart") then
			char.HumanoidRootPart.Anchored = false
			Anchored = false
		end
	end
end)

-- Tüm diğer oyuncuların parçalarını sürekli geçirgen yap (CanCollide = false)
RunService.RenderStepped:Connect(function()
	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character then
			for _, part in ipairs(player.Character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.CanCollide = false
				end
			end
		end
	end
end)


local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer

local function checkGunAndReset()
    local character = LocalPlayer.Character
    local backpack = LocalPlayer:FindFirstChild("Backpack")

    if not character or not backpack then return end

    -- Karakterte Gun varsa
    if character:FindFirstChild("Gun") then
        character:BreakJoints()
        return
    end

    -- Sırt çantasında Gun varsa
    if backpack:FindFirstChild("Gun") then
        character:BreakJoints()
    end
end


RunService.RenderStepped:Connect(checkGunAndReset)


LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    checkGunAndReset()
end)


wait(0.5)

-- Servisler
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local VirtualUser = game:GetService("VirtualUser")

local LocalPlayer = Players.LocalPlayer

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "UltimateAntiAFK"
gui.ResetOnSpawn = false
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
pcall(function() gui.Parent = game:GetService("CoreGui") end)

-- Header
local header = Instance.new("TextLabel")
header.Name = "Header"
header.Parent = gui
header.BackgroundColor3 = Color3.new(0, 0, 0.4)
header.Position = UDim2.new(0.7, 0, 0.1, 0)
header.Size = UDim2.new(0, 400, 0, 50)
header.Font = Enum.Font.Fantasy
header.Text = "Ultimate Anti-AFK"
header.TextColor3 = Color3.new(1, 1, 1)
header.TextSize = 24
header.BorderSizePixel = 0

-- Frame
local frame = Instance.new("Frame")
frame.Name = "MainFrame"
frame.Parent = header
frame.BackgroundColor3 = Color3.new(0, 0, 0.3)
frame.Position = UDim2.new(0, 0, 1, 0)
frame.Size = UDim2.new(0, 400, 0, 100)
frame.BorderSizePixel = 0

-- Status Label
local status = Instance.new("TextLabel")
status.Name = "Status"
status.Parent = frame
status.BackgroundColor3 = Color3.new(0, 0, 0.5)
status.Position = UDim2.new(0, 0, 0.2, 0)
status.Size = UDim2.new(0, 400, 0, 40)
status.Font = Enum.Font.Gotham
status.Text = "Status: Active | AFK avoided: 0"
status.TextColor3 = Color3.new(0.8, 1, 0.8)
status.TextSize = 20
status.BorderSizePixel = 0

-- Footer
local footer = Instance.new("TextLabel")
footer.Name = "Footer"
footer.Parent = frame
footer.BackgroundColor3 = Color3.new(0, 0, 0.4)
footer.Position = UDim2.new(0, 0, 0.8, 0)
footer.Size = UDim2.new(0, 400, 0, 20)
footer.Font = Enum.Font.GothamBold
footer.Text = "Made by Genesis_ui"
footer.TextColor3 = Color3.new(0.8, 0.8, 1)
footer.TextSize = 16
footer.BorderSizePixel = 0

-- Close Button
local closeBtn = Instance.new("TextButton")
closeBtn.Name = "Close"
closeBtn.Parent = header
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 10)
closeBtn.Text = "X"
closeBtn.BackgroundColor3 = Color3.new(0.6, 0, 0)
closeBtn.TextColor3 = Color3.new(1, 1, 1)
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 18
closeBtn.BorderSizePixel = 0

closeBtn.MouseButton1Click:Connect(function()
	gui:Destroy()
end)

-- Sürüklenebilirlik
local dragging = false
local dragStart, startPos

header.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = header.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
		local delta = input.Position - dragStart
		header.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)

-- Anti-AFK Sistemi
local afkCount = 0
LocalPlayer.Idled:Connect(function()
	VirtualUser:CaptureController()
	VirtualUser:ClickButton2(Vector2.new())

	afkCount += 1
	status.Text = "AFK Engellendi! Toplam: "..afkCount
	wait(2)
	status.Text = "Status: Active | AFK avoided: "..afkCount
end)

loadstring(game:HttpGet('https://raw.githubusercontent.com/FilteringEnabled/NamelessAdmin/main/Source'))()
