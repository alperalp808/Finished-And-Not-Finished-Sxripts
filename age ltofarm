local VirtualUser = game:GetService("VirtualUser")
local RunService = game:GetService("RunService")
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

getgenv().farmtank = false
getgenv().AntiKnockback = false
getgenv().clip = false
getgenv().lip = false

wait(3)

--// Services

--// İsimler
local MAIN_NAME = "epicgmingXD_4"

Farm_A = "5lpx4"
Farm_B = "YourNameIsHereXIII"
Farm_C = "Syn_x8"
Farm_D = "dahoodianeznga"
Farm_E = "SrtaAbril_YT"

Noclip = false
LoopTP = false
NoKnock = false
AntiFling = false
KillAura = false

if game.Players.LocalPlayer.Name == Farm_A or Farm_B or Farm_C or Farm_D or Farm_E then
LoopTP = true
NoKnock = true
AntiFling = true
Noclip = true
elseif LocalPlayer.Name  == MAIN_NAME then
LoopTP = true
NoKnock = true
AntiFling = true
Noclip = true
KillAura = true
end



local OrionLib = loadstring(game:HttpGet(("https://raw.githubusercontent.com/jensonhirst/Orion/main/source")))()
local Window =
    OrionLib:MakeWindow(
    {Name = "$hit Talker's Afk Farm Gui", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}
)

local x = Instance.new("Part", game.Workspace)
x.Anchored = true
x.Name = "Nothing"
x.Transparency = 0.8
x.Position =
    Vector3.new(
    10657.6113,
    1434.63904,
    552.198975,
    0.256518424,
    0.905121565,
    0.339047521,
    -0.0115446523,
    -0.347890735,
    0.937463999,
    0.96647042,
    -0.244390965,
    -0.078791067
)
x.Size = Vector3.new(250, 2, 250)

local Tab =
    Window:MakeTab(
    {
        Name = "Tank Farming",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    }
)

local Section =
    Tab:AddSection(
    {
        Name = "Main Account"
    }
)

Tab:AddParagraph(
    "Kullanım Klavuzu ve Menü Bilgilendirmesi",
    "Farm Place Diyer Oyuncuların Bulamayacağı Normal Haritanın Dışındaki Özel bir yerdir Go To Farm Place Tuşu Oyuncuyu Oraya Işınlar.(Farm Burada Gerçekleşir)  Kodun İçinde Çalıştığı Doğrulanmış Bir Anti Afk Scripti Var Menu Açılırken Yüklenir Ve Sonradan Kapatılamaz Kill Aura Ozel Olarak Modifiye Edildi Menzil İçindeki Oyuncular İçin ForceField Taramasi Yapar Ve Oyunculardaki ForceField Silinene Kadar Başlamaz Anti-Knock Anti-Fling Ve Noclip Oyuncuların Herhangi Bir Şekilde Sarsılmasını Engelleyebilmek İçin Eklendi Kullanılmasını Tavsiye Ederim Kod Her Ne Kadar Mükemmel Olsada Main Hesap ve Tank Hesaplar Arasındaki Seviye Farkı yada (Android İçin Geçerli) Executer Konsolunda Biriken Loglar FPS Ve Farm Kalitesini Düsürebilir"
)

local Section =
    Tab:AddSection(
    {
        Name = "Main Account"
    }
)

Tab:AddToggle(
    {
        Name = "Kill Aura",
        Default = KillAura,
        Callback = function(t)
            if t == true then
                getgenv().killaura = true
                while getgenv().killaura do
                    pcall(
                        function()
                            local Players = game.Players
                            local LocalPlayer = Players.LocalPlayer
                            local char = LocalPlayer.Character
                            local hrp = char and char:FindFirstChild("HumanoidRootPart")
                            if not hrp then
                                return
                            end
                            local localPos = hrp.Position
                            local shouldAttack = true
                            local foundTarget = false
                            for _, player in ipairs(Players:GetPlayers()) do
                                if player ~= LocalPlayer then
                                    local pChar = player.Character
                                    local pHRP = pChar and pChar:FindFirstChild("HumanoidRootPart")
                                    if pHRP then
                                        local dist = (localPos - pHRP.Position).Magnitude
                                        if dist <= 8 then
                                            foundTarget = true
                                            if pChar:FindFirstChild("ForceField") then
                                                shouldAttack = false
                                                break
                                            end
                                        end
                                    end
                                end
                            end
                            if foundTarget and shouldAttack then
                                for i = 1, 10 do
                                    game:GetService("ReplicatedStorage").Events.Punch:FireServer(0.4, 0.01, 1)
                                end
                            end
                        end
                    )
                    wait(0.1)
                end
            else
                getgenv().killaura = false
            end
        end
    }
)

Tab:AddButton(
    {
        Name = "Go To Farm Place",
        Callback = function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(10645, 1440, 560)
        end
    }
)

Tab:AddButton(
    {
        Name = "Kill Codex (Menuyude Siler)",
        Callback = function()
            spawn(
                function()
                    while true do
                        for _, gui in pairs(CoreGui:GetChildren()) do
                            local function findCodex(obj)
                                if obj.Name == "Codex" then
                                    return true
                                end
                                for _, child in pairs(obj:GetChildren()) do
                                    if findCodex(child) then
                                        return true
                                    end
                                end
                                return false
                            end
                            if findCodex(gui) then
                                gui:Destroy()
                                print("Codex Deleted")
                            end
                        end
                        wait(1)
                    end
                end
            )
        end
    }
)

local Section =
    Tab:AddSection(
    {
        Name = "Tank Account"
    }
)

Tab:AddToggle(
    {
        Name = "LoopTeleport",
        Default = LoopTP,
        Callback = function(x)
            getgenv().farmtank = x
        end
    }
)

Tab:AddToggle(
    {
        Name = "Noclip",
        Default = Noclip,
        Callback = function(Value)
            getgenv().clip = Value
            if getgenv().clip == true then
                analloop =
                    game:GetService("RunService").Stepped:Connect(
                    function()
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            if v:IsA("BasePart") and v.CanCollide == true then
                                v.CanCollide = false
                            end
                        end
                    end
                )
            elseif getgenv().clip == false and NoclipLoop then
                analloop:Disconnect()
            end
        end
    }
)

Tab:AddToggle(
    {
        Name = "Anti Fling",
        Default = AntiFling,
        Callback = function(Value)
            getgenv().lip = Value
            if getgenv().lip == true then
                NoclipLoop =
                    game:GetService("RunService").Stepped:Connect(
                    function()
                        for _, player in pairs(game.Players:GetPlayers()) do
                            if player ~= game.Players.LocalPlayer and player.Character then
                                for _, part in pairs(player.Character:GetChildren()) do
                                    if part:IsA("BasePart") and part.CanCollide == true then
                                        part.CanCollide = false
                                    end
                                end
                            end
                        end
                    end
                )
            elseif getgenv().lip == false and NoclipLoop then
                NoclipLoop:Disconnect()
            end
        end
    }
)

Tab:AddToggle(
    {
        Name = "Anti Knock",
        Default = NoKnock,
        Callback = function(Value)
            getgenv().AntiKnockback = Value
        end
    }
)



local LastPosition
local connection
connection =
    RunService.Heartbeat:Connect(
    function()
        if not getgenv().AntiKnockback then
            if connection then
                connection:Disconnect()
                connection = nil
            end
            return
        end
        local character = player.Character
        if not character then
            return
        end
        local root = character:FindFirstChild("HumanoidRootPart")
        if not root then
            return
        end
        local linVel = root.AssemblyLinearVelocity.Magnitude
        local angVel = root.AssemblyAngularVelocity.Magnitude
        if linVel > 250 or angVel > 250 then
            root.AssemblyLinearVelocity = Vector3.zero
            root.AssemblyAngularVelocity = Vector3.zero
            if LastPosition then
                root.CFrame = LastPosition
            end
        elseif linVel < 50 and angVel < 50 then
            LastPosition = root.CFrame
        end
    end
)

Players.LocalPlayer.Idled:Connect(
    function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
)

game:GetService("RunService").Stepped:Connect(
    function()
        if getgenv().farmtank == true then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(10645, 1440, 560)
        end
    end
)

game:GetService("RunService").Stepped:Connect(
    function()
        game:GetService("Lighting").ClockTime = 12
    end
)

  game.ClientStorage.Effects.Shield:Destroy()
  game.ClientStorage.Effects.Speed:destroy()
  game.Workspace.Effects:destroy() 

